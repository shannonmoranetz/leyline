{
  "expo": {
    "name": "Leyline",
    "slug": "leyline",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A simple Magic the Gathering life tracking app with a clean user interface. Currently in development with regular future updates planned.",
    "githubUrl": "https://github.com/shannonmoranetz/leyline"
  }
}